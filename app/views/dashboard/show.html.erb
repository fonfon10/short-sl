<p id="notice"><%= notice %></p>

<h1>DASHBOARD</h1>





<table class="table table-striped table-responsive">
  <thead>
    <tr>
      <th>URL</th>
      <th>times accessed</th>
      <th>last accessed</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @links.each do |links| %>
      <tr>

        

        <td><%= links.url %></td>

        <td><%= links.use_count %></td>
        <% if links.use_count > 0 %>
          <td><%= links.updated_at.strftime("%F") %></td>
          <% else %>
            <td></td>
        <% end %>



      <% if links.engineer != nil %>

          <td><%= links.engineer.firstname %></td>
          <td><%= links.engineer.lastname %></td>

          <% else %>
            <td></td>
      <% end %>


      <% if links.engineer.company != nil %>

          <td><%= links.engineer.company.name %></td>
          <% else %>
            <td></td>
      <% end %>

    



      </tr>
    <% end %>
  </tbody>
</table>


<div class="apple_pagination">
    <div class="page_info">
      <%= page_entries_info @links %>
    </div>
    <%= will_paginate @links, :container => false %>
  </div>


<br>
